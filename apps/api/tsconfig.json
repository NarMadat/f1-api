{
  "compilerOptions": {
    "incremental": true,
    "composite": true,
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": "./",
    "rootDir": "src",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["src/*"],
      "*": ["node_modules/*"]
    },
    "resolveJsonModule": true,
    "resolvePackageJsonImports": true,
    "typeRoots": ["./node_modules/@types/"],
    "strict": true,
    "noImplicitAny": true,
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "dist"],
  "tsc-alias": {
    "resolveFullPaths": true,
    "verbose": false
  }
}
